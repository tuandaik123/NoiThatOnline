<?xml version="1.0" encoding="utf-8"?><Database Name="dbNoiThat" Class="dbNoiThatOnlineDataContext" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Table Name="dbo.admin" Member="admins">
